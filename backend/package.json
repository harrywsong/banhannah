{
  "name": "yewon-backend",
  "version": "1.0.0",
  "description": "backend for banhannah",
  "main": "server.js",
  "scripts": {
    "start": "node -r dotenv/config server.js dotenv_config_path=.env.production",
    "dev": "nodemon -r dotenv/config server.js dotenv_config_path=.env.development",
    "dev:debug": "NODE_ENV=development DEBUG=* nodemon -r dotenv/config server.js dotenv_config_path=.env.development",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hls.js": "^1.6.15",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.8.0"
  }
}
